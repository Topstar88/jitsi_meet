{
  "name": "react-addons-text-content",
  "version": "0.0.4",
  "description": "Like DOM API's `Node.textContent` and it works in React Element",
  "main": "dist/react-addons-text-content.js",
  "scripts": {
    "dev": "webpack-dev-server --inline --hot --content-base=dev",
    "build": "NODE_ENV=production babel-node ./scripts/build",
    "test": "nyc ava -v",
    "test.report": "nyc report --reporter=html && open coverage/index.html",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "ava": {
    "babel": "inherit",
    "require": [
      "babel-core/register"
    ],
    "files": [
      "test/*.spec.js"
    ]
  },
  "nyc": {},
  "keywords": [
    "react",
    "addon",
    "addons",
    "text",
    "content"
  ],
  "author": "Rocky Wu <rocky823@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.15.1",
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-strict-mode": "^6.8.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "chalk": "^1.1.3",
    "coveralls": "^2.11.9",
    "eslint": "^2.11.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-rename": "^1.2.2",
    "gulp-uglify": "^1.5.3",
    "nyc": "^6.4.4",
    "react": "^15.1.0",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rwu823/react-addons-text-content.git"
  },
  "bugs": {
    "url": "https://github.com/rwu823/react-addons-text-content/issues"
  },
  "homepage": "https://github.com/rwu823/react-addons-text-content#readme"
}
